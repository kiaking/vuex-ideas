{
  "private": true,
  "name": "vuex",
  "version": "0.1.0",
  "scripts": {
    "dev": "node examples/server.js",
    "build": "node scripts/build.js",
    "lint": "prettier --check --write --parser typescript \"**/*.ts\"",
    "lint:fail": "prettier --check --parser typescript \"**/*.ts\"",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest --testPathIgnorePatterns __tests__/e2e",
    "test:e2e": "start-server-and-test dev http://localhost:8080 'jest --testPathIgnorePatterns __tests__/unit'",
    "coverage": "yarn test:unit --collect-coverage"
  },
  "peerDependencies": {
    "vue": "3.0.0-beta.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/puppeteer": "^2.0.1",
    "@vue/compiler-sfc": "3.0.0-beta.2",
    "brotli": "^1.3.2",
    "chalk": "^4.0.0",
    "execa": "^4.0.0",
    "express": "^4.17.1",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "puppeteer": "^3.0.0",
    "rollup": "^2.6.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "start-server-and-test": "^1.11.0",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "vue": "3.0.0-beta.2",
    "vue-loader": "^16.0.0-alpha.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0"
  }
}
