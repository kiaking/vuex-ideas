{
  "private": true,
  "name": "vuex",
  "version": "0.1.0",
  "scripts": {
    "dev": "node examples/server.js",
    "build": "node scripts/build.js",
    "lint": "prettier --check --write --parser typescript \"**/*.ts\"",
    "lint:fail": "prettier --check --parser typescript \"**/*.ts\"",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest --testPathIgnorePatterns __tests__/e2e",
    "test:e2e": "start-server-and-test dev http://localhost:8080 'jest --testPathIgnorePatterns __tests__/unit'",
    "coverage": "yarn test:unit --collect-coverage"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/puppeteer": "^5.4.0",
    "@vue/compiler-sfc": "^3.0.2",
    "brotli": "^1.3.2",
    "chalk": "^4.1.0",
    "execa": "^4.1.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "prettier": "2.0.4",
    "puppeteer": "^5.4.1",
    "rollup": "^2.33.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "start-server-and-test": "^1.11.5",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.9",
    "typescript": "^4.0.5",
    "vue": "^3.0.2",
    "vue-loader": "^15.9.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0"
  }
}
