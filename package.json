{
  "private": true,
  "name": "vuex",
  "version": "0.1.0",
  "scripts": {
    "build": "node scripts/build.js",
    "lint": "prettier --check --write --parser typescript \"**/*.ts\"",
    "lint:fail": "prettier --check --parser typescript \"**/*.ts\"",
    "test": "yarn lint && yarn coverage",
    "test:fail": "yarn lint:fail && yarn coverage",
    "test:unit": "jest --forceExit --detectOpenHandles",
    "coverage": "yarn test:unit --collect-coverage"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@vue/compiler-sfc": "^3.0.2",
    "brotli": "^1.3.2",
    "chalk": "^4.1.0",
    "execa": "^4.1.0",
    "jest": "^26.6.3",
    "prettier": "2.0.4",
    "rollup": "^2.33.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5",
    "vue": "^3.0.2"
  }
}
